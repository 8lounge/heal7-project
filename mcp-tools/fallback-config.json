{
  "fallback_order": {
    "description": "메모리/IO 효율성 기준 폴백 순서 (1=최우선, 5=최후순위)",
    "tools": [
      {
        "priority": 1,
        "name": "playwright",
        "description": "가장 효율적, 이미 설치됨",
        "memory_usage": "low",
        "io_usage": "low",
        "startup_time": "fast"
      },
      {
        "priority": 2,
        "name": "puppeteer",
        "description": "Node.js 기반, Chromium 사용",
        "memory_usage": "medium",
        "io_usage": "medium",
        "startup_time": "fast"
      },
      {
        "priority": 3,
        "name": "selenium",
        "description": "다양한 브라우저 지원, 상대적으로 무거움",
        "memory_usage": "high",
        "io_usage": "high",
        "startup_time": "slow"
      },
      {
        "priority": 4,
        "name": "skyvern",
        "description": "AI 기반, 가장 리소스 집약적",
        "memory_usage": "high",
        "io_usage": "high",
        "startup_time": "slow"
      }
    ]
  },
  "shared_captures": {
    "path": "/home/ubuntu/mcp-tools/shared-captures",
    "naming_convention": "YYYY-MM-DD_HH-mm-ss_{tool}_{description}",
    "max_age_days": 30,
    "auto_cleanup": true
  },
  "integration": {
    "crawling_service_port": 8003,
    "claude_cli_integration": true,
    "fallback_timeout_seconds": 30,
    "retry_count": 3
  }
}