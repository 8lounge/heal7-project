{
  "name": "playwright", 
  "type": "local",
  "path": "/usr/local/bin/python3 -m playwright",
  "description": "Playwright - 가장 효율적인 브라우저 자동화 (이미 설치됨)",
  "auth_required": false,
  "priority": 1,
  "capture_path": "/home/ubuntu/mcp-tools/shared-captures",
  "features": [
    "multi_browser_support",
    "fast_execution", 
    "screenshot_capture",
    "network_interception",
    "mobile_simulation"
  ],
  "resource_usage": {
    "memory": "low",
    "io": "low",
    "startup_time": "fast"
  },
  "integration": {
    "crawling_service": true,
    "claude_cli": true,
    "existing_installation": true
  }
}